  "scripts": {
    "start": "webpack serve --open --mode development",
    "dev": "webpack --mode development",
    "build": "webpack --mode production",
    "prettier": "prettier --write './**/*.{js,jsx}' --config ./.prettierrc.json",
    "lint": "eslint --debug src/",
    "lint:write": "eslint --debug src/ --fix",
    "format": "yarn prettier && eslint src --ext .js,.jsx --fix && stylelint \"src/**/*.scss\" --fix",
    "check-format": "yarn prettier --check \"./**/*.{js,jsx}\" && yarn lint:write --quiet && stylelint \"src/**/*.scss\" --quiet"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint-staged && yarn test",
      "pre-push": "yarn lint-staged && yarn test"
    }
  },
  "lint-staged": {
    "*.{js,jsx,scss}": [
      "git diff --cached --name-only --diff-filter=ACMRTUXB | grep -E '\\.(js|jsx|scss)$' | xargs yarn check-format"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
